<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/css/bootstrap.min.css"
          onerror="this.href='@Assets["lib/bootstrap/dist/css/bootstrap.min.css"]'; this.onerror=null;"
          integrity="sha384-LN+7fdVzj6u52u30Kp6M/trliBMCMKTyK833zpbD+pXdCLuTusPj697FH4R/5mcr" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.13.1/font/bootstrap-icons.min.css">
    <link rel="stylesheet" href="@Assets["app.css"]" />
    <link rel="stylesheet" href="@Assets["NexusUserTest.Client.styles.css"]" />
    <link rel="stylesheet" href="@Assets["css/variables.css"]" />
    <link href="_content/BlazorPro.Spinkit/spinkit.min.css" rel="stylesheet" />
    <link href="_content/NexusUserTest.Shared/css/nexus-information.css" rel="stylesheet" />
    <ImportMap />
    <link rel="icon" type="image/png" href="favicon.png" />
    <HeadOutlet @rendermode="InteractiveServer" />
</head>

<body>
    <Routes @rendermode="InteractiveServer" />
    <NexusNotificationHub @rendermode="InteractiveServer" Position="NexusNotificationPosition.BottomRight" Timeout="5"
                          InfoIconClass="bi bi-info-circle-fill" SuccessIconClass="bi bi-check-circle-fill"
                          ErrorIconClass="bi bi-exclamation-circle-fill" WarningIconClass="bi bi-exclamation-triangle-fill" />
    <NexusDialog @rendermode="InteractiveServer" />
    <script src="_framework/blazor.web.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/js/bootstrap.bundle.min.js"
            onerror="this.src='@Assets["lib/bootstrap/dist/js/bootstrap.bundle.min.js"]'; this.onerror=null;"
            integrity="sha384-ndDqU0Gzau9qJ1lfW4pNLlhNTkCfHzAVBReH9diLvGRem5+R9g2FzA8ZGN954O5Q" crossorigin="anonymous"></script>
</body>

</html>
